{
    "extends": "next/core-web-vitals",
    "plugins": [
        "unused-imports"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "spaced-comment": "error",
        "quotes": [
            "error",
            "double"
        ],
        "no-duplicate-imports": "error",
        "unused-imports/no-unused-imports": "error",
        "no-unused-vars": "error"
    }
}
